---
import enterprises from "../../../info/enterprises";
---

<article class="flex flex-col items-center gap-[32px]">
  <h2 class="text-[48px] font-bold">Empresas aliadas</h2>
  <ul class="flex justify-center gap-[32px]">
    {
      enterprises.map((info) => {
        return (
          <li>
            <a href="/" class="flex justify-center align-center">
              <img src={info.photo} alt="" width="50%" height="50%" />
            </a>
          </li>
        );
      })
    }
  </ul>
</article>
